name: "GoogleNet_CUB_200_2011_Contrastive_Plus_HDC"

# set the train data input information
layer {
  name: "cub_200_2011"
  type: "ImageData"
  top: "data"
  top: "label"
  include{
    phase: TRAIN
  }
  transform_param {
    crop_size: 224
    mean_value: 104
    mean_value: 117
    mean_value: 123
  }
  image_data_param {
    source: "../cub_bird_train_c10.txt"
    batch_size: 100   
    new_height: 256
    new_width: 256
    # You can choose the folder of the images cropped with bounding boxes or the original images
    root_folder: "D:\\users\\v-yuhyua\\fromGPU02\\data\\cub_200\\CUB_200_2011\\CUB_200_2011\\images\\"
    #"D:\\users\\v-yuhyua\\fromGPU02\\data\\cub_200\\CUB_200_2011\\CUB_200_2011\\images_box\\"
  }
}

